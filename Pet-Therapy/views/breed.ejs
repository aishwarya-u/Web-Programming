<div class="container">
	<div class="row">
		<% for(var i=0; i<dogs.length; i++) { %>
		<div class="col-md-4">
			<a href="/dog/<%= dogs[i]._id %>">
				<div class="thumbnail">
					<img src="<%= dog[i].image %>">
					<div class="caption">
						<p><%= dogs[i].price %></p>
						<p><%= dogs[i].breed.name %></p>
						<p><%= dogs[i].name %></p>
					</div>
				</div>
			</a>
		</div>
		<% } %>
	</div>

</div>